{
  "name": "node_typeorm",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only --exit-child src/app.ts",
    "build": "tsc -p .",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "yarn typeorm migration:generate ./src/migrations/default-entity -d ./src/utils/data-source.ts",
    "migration:run": "yarn typeorm migration:run -d src/utils/data-source.ts",
    "migrate": "rm -rf build && yarn build && yarn typeorm migration:generate ./src/migrations/added-user-entity -d ./src/utils/data-source.ts",
    "db:push": "rm -rf build && yarn build && yarn typeorm migration:run -d src/utils/data-source.ts",
    "seed": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d src/utils/data-source.ts"
  },
  "dependencies": {
    "@jm18457/kafkajs-msk-iam-authentication-mechanism": "^3.1.2",
    "@serialport/parser-readline": "^12.0.0",
    "axios": "^1.7.2",
    "body-parser": "^1.19.1",
    "config": "^3.3.7",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "envalid": "^7.3.1",
    "express": "^4.18.1",
    "express-rate-limit": "^7.3.1",
    "kafkajs": "^2.2.4",
    "ml-regression-multivariate-linear": "^2.0.4",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "serialport": "^12.0.0",
    "typeorm": "0.3.6",
    "typeorm-extension": "^3.5.1",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@types/config": "^0.0.41",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.10",
    "@types/serialport": "^8.0.5",
    "morgan": "^1.10.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.5.2"
  }
}
